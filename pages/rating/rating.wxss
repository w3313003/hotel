@import ".././success/success.wxss";
.wrap {
    width: 690rpx;
    margin: 0 auto;
}

.msg {
    padding: 0 30rpx;
    padding-bottom: 30rpx;
}

.star {
    width: 28rpx;
    height: 28rpx;
    padding: 0 4rpx;
}

.content {
    padding: 0 30rpx;
}

.c-row {
    display: flex;
    height: 70rpx;
    line-height: 70rpx;
}

.title {
    font-size: 28rpx;
    align-self: center;
    color: #666;
}

.id {
    color: #004483;
}

.text-wrap {
    width: 100%;
    box-sizing: border-box;
    padding: 10rpx;
    background: #f5f5f5;
}

.textarea {
    font-size: 28rpx;
    width: 100%;
}
.confirm{
    background: #d61616;
}